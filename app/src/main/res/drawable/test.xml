<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100" width="500" height="500">
  <circle cx="90" cy="50" r="10" fill="green">
    <animate attributeName="cx" values="90; 10; 90" dur="3s" repeatCount="1"/>
    <animate attributeName="cy" values="50; 90; 50" dur="3s" repeatCount="1" calcMode="spline" keySplines="0.5 0.1 0.3 1; 0.5 0.1 0.3 1"/>
  </circle>
</svg>